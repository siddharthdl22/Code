// Print the Fibonacci series up to a certain maximum elements which is given as input

public static void Fibonacci(int n)
	{
		int[] arr = new int[n];
		int counter = 0;
		while(counter < n)
		{
			if(counter == 0)
				arr[counter] = 1;
			else if(counter == 1)
				arr[counter] = 1;
			else
				arr[counter] = arr[counter - 1] + arr[counter - 2];
			counter++;
		}
		for(int i = 0; i < n; i++)
		{
			System.out.println(arr[i]);
		}
	}
