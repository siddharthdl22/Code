// Print the sum of all the divisors of the given number

class MyCalculator
	{
	    public int divisorSum(int n)
	    {
	        int sum = 0;
	        ArrayList<Integer> al = new ArrayList<Integer>();
	        for(int i = 1; i <= n; i++)
	        {
	            if(n % i == 0)
	                al.add(i);
	        }
	        for(int i = 0; i < al.size(); i++)
	        {
	            sum = sum + al.get(i);
	        }
	        return sum;
	    }
	}
