// Input the sorted array and the search element
// Out the index where the search element exists otherwise return -1

	public static int BinarySearch(int[] arr, int n)
	{
		int rs = -1;
		int start = 0;
		int end = arr.length - 1;
		int mid = 0;  
		while(start < end)
		{
			mid = (start + end)/2;
			if(arr[mid] ==  n)
			{
				rs = mid;
				break;
			}
			else if(arr[mid] > n)
				end = mid - 1;
			else if(arr[mid] < n)
				start = mid + 1;
		}
		return rs;
	}
